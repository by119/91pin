page-cart {
        
    ion-app.ios [padding] .scroll-content {
        padding: 0;
        background-color: #F7F7F7;
    }
    .cart-box {
        margin: 1rem 0;
    }
    .cart-list {
        width: 100%;
        border-bottom: 0;
        background-color: #fff;
    }
    .cart-list .item-ios {
        border-bottom: none;
    }
    .list-title {
        font-size: 1.6rem;
        line-height: 3rem;
        padding:0 5%!important;
    }
    .list-note {
        font-size: 1.2rem;
        line-height: 3rem;
        padding:0 5%!important;
    }
    .list-content {
        padding: 1rem 5%!important;
    }
    .list-content .item-ios {
        border-bottom: none;
    }
    .list-content p{
        margin: 0;
    }
    .list-content ion-note{
        
    }
    .list-img {
        width: 30%;
        vertical-align: middle;
    }
    .list-detail {
        width: 60%;
        float: right;
    }
    .list-detail ion-label {
        overflow: hidden;
        white-space:initial;
    }
    .list-detail p{
        // width: 100%;
        font-size: 1.4rem;
        // overflow: hidden;
        // white-space: nowrap;
        // text-overflow: ellipsis;
        
    }
    .list-detail .list-price {
        // padding-right: 1rem;
        color: #FE5261;
    }

    .list-price span {
        font-size: 1rem;
    }
    .list-detail ion-note{
        font-size: 1rem;
    }
    // 购物车为空
    .cart-null {
        text-align: center;
        color: #c6c6c6;
        line-height: 10rem;
    }


    // 可能喜欢
    .like-box {
        background-color: #fff;
        padding-bottom: 50px;
    }
    .like-box ion-title{
        line-height: 4rem;
        border-bottom: 1px solid #F7F7F7;
        margin-bottom: 1rem;
    }
    .like-list {
        width: 44%;
        margin: 2% 0;
        float: left;
    }

    .like-list:nth-child(2n+1){
        margin-left: 4%;
    }
    .like-list:nth-child(2n){
        margin-left: 3%;
    }
    .like-list ion-label {
        overflow: hidden;
        white-space:initial;
    }
    .like-list .like-depict {
        color: #333;
        font-size: 1rem;
        line-height: 1.6rem;
        margin: 0;
        overflow : hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
    }
    .like-list .like-price {
        color: #FE5261;
        line-height: 1.4rem;
        margin: .5rem 0;
    }
    .like-price span {
        font-size: 1rem;
    }

    .may-like .item-ios {
        border-bottom: none;
    }
    // 购物车底部
    .submit {
        position: fixed;
        bottom: 50px;
        width: 100%;
        font-size: 1.6rem;
        background-color: #fff;
        .show-btn {
            width: 80%;
            float: left;
            background-color: #fff;
            p {
                margin: 0;
                padding-left: 1rem;
            }
            p:first-child {
                color: #ec5151;
            }
            p:last-child {
                color: #777;
                font-size: 1rem;
            }
        }
        .pay-btn {
            width:20%;
            float: right;
            color: #fff;
            background-color: #ec5151;
            text-align: center;
            line-height: 3.6rem;
        }
    }
}