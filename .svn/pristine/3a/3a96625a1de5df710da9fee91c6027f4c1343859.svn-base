page-redbag {
    .coupon-list {
        height: 5rem;
        margin: .5rem .5rem 0 .5rem;
        background-color: #fff;
        background-repeat: no-repeat;
        background-size: 100%;
        border-radius: .5rem;
        overflow: hidden;
        .list-l {
            float: left;
            padding: .5rem;
            .list-l-t {
                .list-price {
                    float: left;
                    font-size: 2.6rem;
                    color: #73D3D6;
                    vertical-align: text-top;
                    margin-right: .3rem;
                    span {
                        float: left;
                    }
                }
                .list-ask {
                    float: left;
                    p {
                        font-size: .8rem;
                        margin: 0;
                    }
                }
            }
            .list-l-b {
                p {
                    font-size: .8rem;
                    margin: 0;
                    color: #7C7E7C;
                    padding-left: .3rem;
                }
            }
        }
        .list-r {
            float: right;
            width: 5rem;
            padding: 1rem;
            line-height: 1.5rem;
            font-size: 1.4rem;
            background-color: #73D3D6;
            text-align: center;
            color: #fff;
        }
    }
}
