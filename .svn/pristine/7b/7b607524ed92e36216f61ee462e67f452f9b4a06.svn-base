    //     z-index: 99;
    // }
    // .collage-list-box {
    //     padding-top: 2.5rem;
    // }
    .collage-list {
        position: relative;
        padding: .5rem 0;
        background-color: #fff;
        margin: .5rem 0;
        img {
            width: 24%;
            float: left;
            padding: 2%;
            vertical-align: middle;
        }
        .collage-info {
            float: left;
            width: 72%;
            padding-left: 2%;
            border: none;
            .collage-price {
                color: #FE5261;
                font-size: 1.4rem;
            }
            p {
                margin: 0 0 .3rem 0;
                overflow: hidden;
                text-overflow: ellipsis;
                display: -webkit-box;
                -webkit-line-clamp: 2;
                -webkit-box-orient: vertical;                            
            }
        }
        .view-order {
            float: right;
            display: inline-block;
            border: 1px solid #b6b6b6;
            color: #333;
            border-radius: .2rem;
            margin-right: .5rem;
            padding: 0 .3rem;
        }
        .progress {
            float: right;
            border: 1px solid #FE5261;
            color: #FE5261;
            border-radius: .2rem;
            padding: 0 .3rem;
        }
        .state-img {
            // width: 20%;
            position: absolute;
            top: 46%;
            transform: translateY(-50%);
            right: .5rem;
            font-size: 6rem;
        }
    }
    .null-order {
        text-align: center;
    }
}
ion-app.ios [padding] .scroll-content {
    padding: 0;
}