    .swiper-slide img {
        width: 100%;
        display: block;
      }
      button.swiper-pagination-bullet{ border-radius: 2px; border: 1px solid white; width: 12px; height: 4px; background-color: transparent; opacity: .8;}
      button.swiper-pagination-bullet-active{ background-color: white;}
  
      .swiper-slide .slide-zoom div{
        padding-top: 14px; padding-top: calc(constant(safe-area-inset-top) - 14px);padding-top: calc(env(safe-area-inset-top) - 14px);
      }
    
      .types{
        background-color: color($colors,background);
        & > div{
          &:first-child{width: calc(48.66% - 0.275px);}
          &:last-child{
            width: calc(51.34% - 0.275px);
            img:first-child{ margin-bottom: 0.55px;}
          }
          img{ width: 100%}
        } 
      }
}
